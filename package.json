{
  "name": "handy-perl-snippets",
  "displayName": "handy-perl-snippets",
  "description": "Add this extension to get access to a bunch of handy perl code snippets",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.87.0"
  },
  "categories": [
    "Snippets"
  ],
  "contributes": {
    "snippets": [
      {
        "language": "perl",
        "path": "./snippets/snippets.code-snippets"
      }
    ]
  }
}
